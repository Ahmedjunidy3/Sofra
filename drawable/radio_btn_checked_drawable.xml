<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="@dimen/dim_3dp"
        android:left="@dimen/dim_3dp"
        android:right="@dimen/dim_3dp"
        android:top="@dimen/dim_3dp">
        <shape android:shape="oval">
            <solid android:color="@color/colorPrimary" />
            <size
                android:width="@dimen/dim_15dp"
                android:height="@dimen/dim_15dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">

            <stroke
                android:width="@dimen/dim_2dp"
                android:color="@color/colorDarkGray" />
            <size
                android:width="@dimen/dim_20dp"
                android:height="@dimen/dim_20dp" />
        </shape>
    </item>
</layer-list>